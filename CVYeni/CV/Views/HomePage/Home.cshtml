@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>Erkan Bostan</title>
    <meta content="" name="description">
    <meta content="" name="keywords">
    <link href="~/Portf/img/Sisarp.png" rel="icon">
    <link href="~/Portf/assets/img/apple-touch-icon.png" rel="apple-touch-icon">
    <link href="~/Portf/assets/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/Portf/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Portf/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/Portf/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/Portf/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/Portf/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">
    <link href="~/Portf/assets/css/style.css" rel="stylesheet">
</head>
<body>
    <header id="header">
        <div class="d-flex flex-column">
            @Html.Action("Links", "HomePage")
            <nav id="navbar" class="nav-menu navbar">
                <ul>
                    <li><a href="#hero" class="nav-link scrollto active"><i class="bx bx-home"></i> <span>Home</span></a></li>
                    <li><a href="#about" class="nav-link scrollto"><i class="bx bx-user"></i> <span>About</span></a></li>
                    <li><a href="#resume" class="nav-link scrollto"><i class="bx bx-file-blank"></i> <span>Resume</span></a></li>
                    <li><a href="#portfolio" class="nav-link scrollto"><i class="bx bx-book-content"></i> <span>Portfolio</span></a></li>
                    <li><a href="#services" class="nav-link scrollto"><i class="bx bx-server"></i> <span>Services</span></a></li>
                    <li><a href="#contact" class="nav-link scrollto"><i class="bx bx-envelope"></i> <span>Contact</span></a></li>
                </ul>
            </nav>
        </div>
    </header>
    @Html.Action("Profiles", "HomePage")
    <main id="main">
        @Html.Action("About", "HomePage")
        @Html.Action("Facts", "HomePage")
        @Html.Action("Skills", "HomePage")
        @Html.Action("Resume", "HomePage")
        @Html.Action("Portfolio", "HomePage")
        @Html.Action("Services", "HomePage")
        @Html.Action("Contact", "HomePage")
    </main>
    @Html.Action("Footer", "HomePage")
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>
    <script src="~/Portf/assets/vendor/purecounter/purecounter.js"></script>
    <script src="~/Portf/assets/vendor/aos/aos.js"></script>
    <script src="~/Portf/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Portf/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/Portf/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/Portf/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/Portf/assets/vendor/typed.js/typed.min.js"></script>
    <script src="~/Portf/assets/vendor/waypoints/noframework.waypoints.js"></script>
    <script src="~/Portf/assets/vendor/php-email-form/validate.js"></script>
    <script src="~/Portf/assets/js/main.js"></script>
</body>
</html>
